a=input("entrez le premier nombre binaire:")
b=input("entre le second nombre binaire:")
def f(x):      #c'est la fonction pour convertir decimale binaire :)#
    xdec=int(0)
    r=int(len(x))-1 #c'est le rang pour l'op√©ration
    for str in x:
        if str=="1":
            xdec=xdec+(1*2**r)
            r=r-1
        else:
            r=r-1
    return(xdec)
print("a=",f(a),"b=",f(b))
